        "lint:check": "eslint \"src/**/\""
    },
    "devDependencies": {
        "@babel/preset-env": "^7.20.2",
        "eslint": "^8.34.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest": "^27.2.1",
        "jest": "^29.4.3",
        "microbundle": "^0.15.1"
    },
    "dependencies": {
        "cross-fetch": "^3.1.5"
    },
    "browserslist": "> 0.25%, not dead"
}